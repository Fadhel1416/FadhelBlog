    $('#alertPartage').css('display','block');

  }
  else if(validURL(url)==false)
  {
   $('#alertPartage').html('You must enter a valid URL');
    document.getElementById('alertPartage').className="alert alert-danger";
    $('#alertPartage').css('display','block');
  }

else {
  $.ajax({
    type:"post",
    url:"{{path('partageRessource')}}",
