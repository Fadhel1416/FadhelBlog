  <script>

function sendMail()
{
  //e.preventDefault();
  var name=$('#name').val();
  var email=$('#email').val();
  var subject=$('#subject').val();
  var message=$('#message').val();
  if(name=="" || email=="" || subject=="" || message=="")
  {
    $('#alertMessage').html('You must fill all the fields');
    document.getElementById('alertMessage').className="alert alert-danger";
    $('#alertMessage').css('display','block');

  }

else {
  $.ajax({
    type:"post",
    url:"{{path('sendMail')}}",
    data:{name:name,email:email,subject:subject,message:message},
    dataType:"json",
    success:function(data){
      $('#name').val('');
      $('#email').val('');
      $('#subject').val('');
      $('#message').val('');
      if(data=="email sent"){
       $('#alertMessage').html('your message has been sent');
       document.getElementById('alertMessage').className="alert alert-success";
       $('#alertMessage').css('display','block');

          }
    }
  });
}

 
}
function validURL(str) {
  var pattern = new RegExp('^(https?:\\/\\/)?'+ // protocol
    '((([a-z\\d]([a-z\\d-]*[a-z\\d])*)\\.)+[a-z]{2,}|'+ // domain name
    '((\\d{1,3}\\.){3}\\d{1,3}))'+ // OR ip (v4) address
    '(\\:\\d+)?(\\/[-a-z\\d%_.~+]*)*'+ // port and path
    '(\\?[;&a-z\\d%_.~+=-]*)?'+ // query string
    '(\\#[-a-z\\d_]*)?$','i'); // fragment locator
  return !!pattern.test(str);
}

function partageRessource(){

  var url=$('#url').val();
  var content=$('#content').val();
  if(url=="" || content=="")
  {
    $('#alertPartage').html('You must fill all the fields');
